<plugin>
   <extension
         point="org.eclipse.chemclipse.chromatogram.msd.classifier.chromatogramClassifierSupplier">
      <ChromatogramClassifierSupplier
            classifier="net.openchrom.xxd.classifier.supplier.ratios.core.PeakIonClassifier"
